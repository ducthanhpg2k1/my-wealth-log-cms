{
  "name": "c-users-pc-documents-thanh-my-wealth-log",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "dev": "env-cmd -f .env.clientDev next dev -p 3333",
    "build": "next build",
    "buildDev": "cp -f .env.clientDev .env && next build",
    "buildProd": "cp -f .env.clientProd .env && next build",
    "start": "env-cmd -f .env.clientProd next start -p 3333",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "verify-commit": "verify-commit-msg",
    "prepare": "git-scm-hooks",
    "release": "bumpp -r",
    "gen-icon": "csvgtocss",
    "gen-color": "esno ./scripts/genColorCss.ts",
    "standalone": "node server.js"
  },
  "dependencies": {
    "@ant-design/icons": "^5.5.1",
    "ahooks": "^3.8.1",
    "antd": "^5.20.3",
    "autoprefixer": "^10.4.20",
    "brainless-token-manager": "^1.3.3",
    "chart.js": "^4.4.4",
    "classnames": "^2.5.1",
    "cookies-next": "^4.2.1",
    "dayjs": "^1.11.13",
    "file-saver": "^2.0.5",
    "jotai": "^2.9.3",
    "next": "^14.2.6",
    "next-i18next": "^11.3.0",
    "nextjs-progressbar": "^0.0.16",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.3.1",
    "sharp": "^0.33.5",
    "sonner": "^1.5.0",
    "umi-request": "^1.4.0",
    "unloger": "^0.0.3"
  },
  "devDependencies": {
    "@ant-design/cssinjs": "^1.21.1",
    "@ant-design/static-style-extract": "^1.0.2",
    "@hunghg255/eslint-config-react": "^0.0.26",
    "@next/bundle-analyzer": "^12.3.4",
    "@total-typescript/ts-reset": "^0.4.2",
    "@types/file-saver": "^2.0.7",
    "@types/node": "18.7.16",
    "@types/react": "18.0.18",
    "@types/react-dom": "18.0.6",
    "bumpp": "^9.5.2",
    "cross-env": "^7.0.3",
    "csvgtocss": "^0.1.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^13.5.6",
    "esno": "^4.7.0",
    "git-scm-hooks": "^0.0.11",
    "prettier": "^2.8.8",
    "sass": "^1.77.8",
    "ts-node": "^10.9.2",
    "tslib": "^2.7.0",
    "typescript": "4.8.3",
    "verify-commit-msg": "^0.0.14"
  },
  "browserslist": [
    ">0.3%",
    "not ie 11",
    "not dead",
    "not op_mini all"
  ],
  "git-hooks": {
    "pre-commit": "npm run lint",
    "commit-msg": "npm run verify-commit"
  }
}
